  "scripts": {
    "start": "cross-env-shell REACT_APP_BUILD_SHA=$GIT_COMMIT react-scripts start",
    "build": "cross-env-shell REACT_APP_BUILD_SHA=$GIT_COMMIT react-scripts build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "build:docker": "env-cmd -f .env.docker npm run build",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "eslint": "eslint --ext js,jsx,ts,tsx . -f html -o ./eslint/eslint.html",
    "eslint:jenkins": "eslint --ext js,jsx,ts,tsx . -f checkstyle -o ./eslint/eslint.xml",
    "eslint:swqat": "eslint --ignore-pattern src/__*__/** --ext js,jsx,ts,tsx . -f html -o ./eslint/eslint-swqat.html",
    "clean": "shx rm -rf build && shx rm -f package-lock.json && npm cache clean -f && shx rm -rf node_modules",
    "rebuild": "npm run clean && npm i"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura"
