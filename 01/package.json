{
  "name": "feathers-starter-ts",
  "description": "Starter project for Feathers App with TypeScript",
  "version": "0.0.0",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "deskoh"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "8.0.0",
    "npm": ">= 3.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.{ts,js}"
    ],
    "extension": [
      ".ts",
      ".js"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "scripts": {
    "coverage": "nyc --extension .ts npm run mocha",
    "test": "npm run eslint && npm run mocha",
    "test:all": "npm run eslint && cross-env NODE_ENV=test npm run mocha",
    "dev": "nodemon src/index.ts",
    "start": "node dist",
    "start:ts": "ts-node -r tsconfig-paths/register --files src/",
    "start:seed": "cross-env NODE_ENV=test ts-node --files src/ --seed",
    "mocha": "cross-env NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json TS_NODE_FILES=true mocha -r ts-node/register \"test/**/*.test.{ts,js}\" --timeout 10000 --exit",
    "mocha:dev": "cross-env NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json TS_NODE_FILES=true mocha -r ts-node/register \"test/services/workspaces/**/*.test.{ts,js}\" --timeout 10000 --exit",
    "build": "tsc -p tsconfig.json",
    "eslint": "eslint --ext ts ."
  },
  "dependencies": {
    "@feathers-plus/test-utils": "0.3.6",
    "@feathersjs/authentication": "4.4.3",
    "@feathersjs/authentication-local": "4.4.3",
    "@feathersjs/configuration": "4.4.3",
    "@feathersjs/errors": "4.4.0",
    "@feathersjs/express": "4.5.0",
    "@feathersjs/feathers": "4.4.3",
    "@feathersjs/socketio": "4.4.3",
    "@feathersjs/transport-commons": "4.5.0",
    "ajv": "6.10.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-env": "6.0.3",
    "feathers-authentication-hooks": "1.0.1",
    "feathers-hooks-common": "4.20.7",
    "feathers-mongoose": "8.3.0",
    "feathers-swagger": "1.1.0",
    "feathers-sync": "1.2.0",
    "helmet": "3.21.2",
    "lodash": "4.17.15",
    "lodash.merge": "4.6.2",
    "moment": "2.24.0",
    "mongoose": "5.8.9",
    "multer": "1.4.2",
    "node-schedule": "1.3.2",
    "passport-custom": "1.1.0",
    "passport-oauth2": "1.5.0",
    "request": "2.88.0",
    "saslprep": "1.0.3",
    "serve-favicon": "2.5.0",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.0"
  },
  "devDependencies": {
    "@feathersjs/client": "4.4.3",
    "@types/chai": "4.2.7",
    "@types/compression": "1.0.1",
    "@types/cors": "2.8.6",
    "@types/helmet": "0.0.45",
    "@types/lodash.merge": "4.6.6",
    "@types/mocha": "5.2.7",
    "@types/mongoose": "5.5.42",
    "@types/multer": "1.3.10",
    "@types/node-schedule": "1.3.0",
    "@types/passport-oauth2": "1.4.8",
    "@types/passport-strategy": "0.2.35",
    "@types/request-promise": "4.1.45",
    "@types/serve-favicon": "2.5.0",
    "@types/sinon": "7.5.1",
    "@typescript-eslint/eslint-plugin": "2.13.0",
    "@typescript-eslint/parser": "2.13.0",
    "axios": "0.19.1",
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-sonarjs": "0.5.0",
    "husky": "4.2.0",
    "jsonfile": "5.0.0",
    "lint-staged": "9.5.0",
    "mocha": "6.2.2",
    "nodemon": "2.0.2",
    "nyc": "15.0.0",
    "request-promise": "4.2.5",
    "shx": "0.3.2",
    "sinon": "8.1.0",
    "socket.io-client": "2.3.0",
    "source-map-support": "0.5.16",
    "ts-mocha": "6.0.0",
    "ts-node": "8.6.2",
    "typescript": "3.7.2"
  }
}
