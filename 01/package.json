{
  "name": "webserver",
  "version": "0.2.0",
  "main": "src",
  "private": true,
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "12.16.3",
    "npm": ">= 3.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.{ts,js}"
    ],
    "extension": [
      ".ts",
      ".js"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  },
  "types": "lib/",
  "scripts": {
    "dev": "ts-node-dev --no-notify src/",
    "debug": "ts-node-dev --inspect --respawn --no-notify --transpileOnly src/",
    "eslint": "eslint --ext ts,js . -f html -o ./eslint/eslint.html",
    "eslint:jenkins": "eslint --ext ts,js . -f checkstyle -o ./eslint/eslint.xml",
    "eslint:swqat": "eslint --ignore-pattern test/** --ext js,jsx,ts,tsx . -f html -o ./eslint/eslint-swqat.html",
    "mocha": "cross-env NODE_ENV=test TS_NODE_PROJECT=test/tsconfig.json TS_NODE_FILES=true ts-mocha -r tsconfig-paths/register \"test/**/*.test.{ts,js}\" --timeout 10000 --recursive --exit",
    "coverage": "nyc --extension .ts --reporter=lcov --reporter=cobertura npm run mocha -- --reporter=xunit --reporter-options output=test.xml",
    "test": "npm run eslint && cross-env NODE_ENV=test npm run mocha",
    "build": "tsc -p tsconfig.json",
    "start": "node dist",
    "clean": "shx rm -rf dist && shx rm -f package-lock.json && npm cache clean -f && shx rm -rf node_modules",
    "rebuild": "npm run clean && npm i"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "4.5.2",
    "@feathersjs/authentication": "4.5.3",
    "@feathersjs/authentication-local": "4.5.3",
    "@feathersjs/configuration": "4.5.3",
    "@feathersjs/errors": "4.5.3",
    "@feathersjs/express": "4.5.4",
    "@feathersjs/feathers": "4.5.3",
    "@feathersjs/socketio": "4.5.3",
    "ajv": "6.12.2",
    "axios": "0.19.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "feathers-authentication-hooks": "1.0.2",
    "feathers-hooks-common": "5.0.3",
    "feathers-mongoose": "8.3.0",
    "feathers-swagger": "1.2.1",
    "feathers-sync": "2.1.0",
    "get-stream": "5.1.0",
    "helmet": "3.22.0",
    "kafka-node": "5.0.0",
    "lodash": "4.17.15",
    "lodash.merge": "4.6.2",
    "moment": "2.24.0",
    "mongoose": "5.9.10",
    "ms": "2.1.2",
    "multer": "2.0.0-rc.2",
    "nanoid": "3.1.3",
    "node-schedule": "1.3.2",
    "saslprep": "1.0.3",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.2"
  },
  "devDependencies": {
    "@feathersjs/authentication-client": "4.5.3",
    "@feathersjs/rest-client": "4.5.4",
    "@feathersjs/socketio-client": "4.5.3",
    "@types/chai": "4.2.11",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.6",
    "@types/helmet": "0.0.47",
    "@types/lodash.merge": "4.6.6",
    "@types/mocha": "7.0.2",
    "@types/mongoose": "5.7.13",
    "@types/ms": "0.7.31",
    "@types/multer": "1.4.3",
    "@types/nanoid": "2.1.0",
    "@types/node-schedule": "1.3.0",
    "@types/sinon": "9.0.4",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-sonarjs": "0.5.0",
    "husky": "4.2.5",
    "jsonfile": "6.0.1",
    "lint-staged": "10.2.6",
    "mocha": "7.2.0",
    "nodemon": "2.0.4",
    "nyc": "15.0.1",
    "shx": "0.3.2",
    "sinon": "9.0.2",
    "socket.io-client": "2.3.0",
    "source-map-support": "0.5.19",
    "ts-mocha": "7.0.0",
    "ts-node": "8.10.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3"
  }
}
