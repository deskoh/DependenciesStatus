{
  "name": "feathers-starter-ts",
  "description": "Starter project for Feathers App with TypeScript",
  "version": "0.0.0",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "deskoh"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": ">= 3.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.{ts,js}"
    ],
    "extension": [
      ".ts",
      ".js"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "scripts": {
    "coverage": "nyc --extension .ts npm run mocha",
    "test": "npm run eslint && npm run mocha",
    "test:all": "npm run eslint && cross-env NODE_ENV=test npm run mocha",
    "dev": "nodemon src/index.ts",
    "start": "node dist",
    "start:ts": "ts-node -r tsconfig-paths/register --files src/",
    "start:seed": "cross-env NODE_ENV=test ts-node --files src/ --seed",
    "mocha": "cross-env NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json TS_NODE_FILES=true mocha -r ts-node/register \"test/**/*.test.{ts,js}\" --timeout 10000 --exit",
    "mocha:dev": "cross-env NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json TS_NODE_FILES=true mocha -r ts-node/register \"test/services/workspaces/**/*.test.{ts,js}\" --timeout 10000 --exit",
    "build": "tsc -p tsconfig.json",
    "eslint": "eslint --ext ts ."
  },
  "dependencies": {
    "@feathers-plus/test-utils": "^0.3.6",
    "@feathersjs/authentication": "^4.3.11",
    "@feathersjs/authentication-jwt": "^2.0.10",
    "@feathersjs/authentication-local": "^4.3.11",
    "@feathersjs/authentication-oauth2": "^1.3.1",
    "@feathersjs/configuration": "^4.3.11",
    "@feathersjs/errors": "^4.3.11",
    "@feathersjs/express": "^4.3.11",
    "@feathersjs/feathers": "^4.3.11",
    "@feathersjs/socketio": "^4.3.11",
    "ajv": "^6.10.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "feathers-hooks-common": "^4.20.7",
    "feathers-mongodb": "^6.0.0",
    "feathers-mongoose": "^8.1.0",
    "feathers-nedb": "^5.1.0",
    "helmet": "^3.15.1",
    "lodash.merge": "^4.6.1",
    "mongodb": "^3.1.13",
    "mongodb-core": "^3.1.11",
    "mongoose": "^5.4.20",
    "nedb": "^1.8.0",
    "passport-custom": "^1.0.5",
    "passport-oauth2": "^1.4.0",
    "request": "^2.88.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@feathersjs/client": "^4.3.11",
    "@types/compression": "1.0.1",
    "@types/cors": "^2.8.4",
    "@types/feathersjs__authentication": "^2.1.4",
    "@types/feathersjs__authentication-jwt": "^1.0.6",
    "@types/feathersjs__authentication-local": "^1.0.4",
    "@types/feathersjs__authentication-oauth2": "^1.0.5",
    "@types/feathersjs__configuration": "^1.0.2",
    "@types/feathersjs__errors": "^3.3.1",
    "@types/feathersjs__express": "^1.1.5",
    "@types/feathersjs__feathers": "^3.1.1",
    "@types/feathersjs__socketio": "^3.0.4",
    "@types/helmet": "0.0.45",
    "@types/lodash.merge": "^4.6.5",
    "@types/mocha": "^5.2.6",
    "@types/mongodb": "^3.1.20",
    "@types/mongoose": "^5.3.24",
    "@types/nedb": "^1.8.7",
    "@types/passport-oauth2": "^1.4.8",
    "@types/request-promise": "^4.1.42",
    "@types/serve-favicon": "^2.2.30",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "axios": "^0.19.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.16.0",
    "jsonfile": "^5.0.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.18.10",
    "nyc": "^14.1.1",
    "request-promise": "^4.2.4",
    "socket.io-client": "^2.2.0",
    "source-map-support": "^0.5.10",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.3.3333"
  }
}
